  <rule id="DEVICE" scope="public">
    <one-of>
      <item >licht</item>
	 <item >lampe</item>
    </one-of>
  </rule>
 
  <rule id="COMMAND" scope="public">
    <one-of>
      <item >ein</item>
	 <item >aus</item>
    </one-of>
  </rule>
  
  <rule id="LIGHT" scope="public">
    <one-of>
	 <item>
	  <ruleref special="GARBAGE"/>
	  <ruleref uri="#DEVICE" />
	  <ruleref special="GARBAGE"/>
	  <ruleref uri="#COMMAND" />
	  <ruleref special="GARBAGE"/>
	 </item>
    </one-of>
  </rule>
</grammar>

